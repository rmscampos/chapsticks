<%- include('../partials/header') %>
<table id="list">
  <tbody>
    <% chapsticks.forEach(function(c) { %>
      <tr>
        <td><%= c.brand %></td>
        <td><%= c.flavor %></td>
        <td><%= c.category %></td>
        <td><%= c.season %></td>
        <td><%= c.review %></td>
        <td><a href="/chapsticks/<%= c._id %>">REVIEWS</a></td>
      </tr>
      <% }); %>
    </tbody>
  </table>
  
<%- include('../partials/footer') %>